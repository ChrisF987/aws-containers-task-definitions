{
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/tomcat-container-logs",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 8080,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "cpu": 1024,
      "memory": 256,
      "image": "tomcat",
      "essential": true,
      "name": "tomcat-webserver"
    }
  ],
  "memory": "2048",
  "family": "tomcat-webserver",
  "networkMode": "awsvpc",
  "cpu": "1024"
}