{  
   "memory":"2048",
   "networkMode":"awsvpc",
   "cpu":"1024",
   "containerDefinitions":[  
      {  
         "portMappings":[  
            {  
               "hostPort":80,
               "containerPort":5601,
               "protocol":"tcp"
            }
         ],
         "essential":true,
         "name":"kibana",
         "environment":[  
            {  
               "name":"ELASTICSEARCH_URL",
               "value":"<ES-URL>"
            }
         ],
         "cpu": 1024,
         "memory": 2048,
         "image":"docker.elastic.co/kibana/kibana:6.0.0",
         "logConfiguration":{  
            "logDriver":"awslogs",
            "options":{  
               "awslogs-group":"/ecs/kibana",
               "awslogs-region":"us-east-1"
            }
         }
      }
   ],
   "family":"kibana"
}