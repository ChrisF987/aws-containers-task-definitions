{  
   "networkMode":"bridge",
   "containerDefinitions":[  
      {  
         "volumesFrom":[  

         ],
         "portMappings":[  
            {  
               "hostPort":80,
               "containerPort":5601,
               "protocol":"tcp"
            }
         ],
         "essential":true,
         "name":"kibana",
         "environment":[  
            {  
               "name":"ELASTICSEARCH_URL",
               "value":"<ES-URL>"
            }
         ],
         "image":"docker.elastic.co/kibana/kibana:6.0.0",
         "logConfiguration":{  
            "logDriver":"awslogs",
            "options":{  
               "awslogs-group":"kibana-logs",
               "awslogs-region":"us-west-2"
            }
         },
         "cpu":1024,
         "memoryReservation":1024
      }
   ],
   "family":"kibana"
}